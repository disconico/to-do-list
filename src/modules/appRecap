import { launchApp } from './launchApp';
import { displayTasks, displayProjects, eventListeners } from './DOMevents';
import { myLibrary, myProjects, restoreTasks, restoreProjects } from './functions';

const mainAppFunction = function () {
    const content = document.createElement('div')
    content.classList.add('content')
    content.setAttribute('id', 'content')

    const body = document.querySelector('body')
    body.appendChild(content)

    launchApp()
    restoreTasks()
    restoreProjects()
    displayTasks('')
    displayProjects()
    eventListeners()
}

export { mainAppFunction }